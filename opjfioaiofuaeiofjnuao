local GetName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)



local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()
local win = Flux:Window("MH HUB",GetName.Name, Color3.fromRGB(255, 110, 48), Enum.KeyCode.RightControl)
local tab = win:Tab("LocalPlayer", "http://www.roblox.com/asset/?id=6023426915")
tab:Slider("Walkspeed", "", 0, 1000,16,function(t)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (t)
end)
tab:Slider("JumpPower", ".", 0, 1000,50,function(t)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = (t)
end)
tab:Slider("Gravity", ".", 0, 1000,196.2,function(t)
game.Workspace.Gravity = (t)
end)
tab:Button("Default", "Make Speed/Jumppower/Grivity normal", function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
game.Workspace.Gravity = 196.2
end)
tab:Button("Noclip", "", function(t)
    _G.PRO = true
local noclip = _G.PRO char = game.Players.LocalPlayer.Character while _G.PRO do if noclip == _G.PRO then for _,v in pairs(char:children()) do pcall(function() if v.className == "Part" then v.CanCollide = false elseif v.ClassName == "Model" then v.Head.CanCollide = false end end) end end game:service("RunService").Stepped:wait() end
end)
tab:Button("Clip", "", function(t)
_G.PRO = false   
end)
