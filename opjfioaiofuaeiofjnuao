local Id = "6741738180" --ENTER THE SOUND ID HERE

Sound.Parent = game.Workspace
Sound.SoundId = "rbxassetid://"..Id
Sound.Playing = true
Sound.Looped = false -- CHANGE TO FALSE IF YOU DON'T WANT IT TO LOOP
Sound.Volume = 5 -- CHANGE THE VOLUME OF THE MUSIC DEFAULT(1)
wait(6.5)
